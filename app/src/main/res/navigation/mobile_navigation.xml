<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/nav_home">

    <fragment
        android:id="@+id/nav_home"
        android:name="com.fictivestudios.orbicons.fragments.HomeFragment"
        android:label="@string/menu_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/nav_apply"
        android:name="com.fictivestudios.orbicons.fragments.InstalledLuancherFragment"
        android:label="Apply"
        tools:layout="@layout/fragment_apply" />

    <fragment
        android:id="@+id/nav_icons"
        android:name="com.fictivestudios.orbicons.fragments.IconsFragment"
        android:label="Icons"
        tools:layout="@layout/fragment_icons" />


    <fragment
        android:id="@+id/nav_terms"
        android:name="com.fictivestudios.orbicons.fragments.TermsFragment"
        android:label="terms &amp; conditions"
        tools:layout="@layout/fragment_terms" />

    <fragment
        android:id="@+id/nav_privacy_policy"
        android:name="com.fictivestudios.orbicons.fragments.PrivacyFragment"
        android:label="Privacy Policy"
        tools:layout="@layout/fragment_privacy_fragment" />

    <fragment
        android:id="@+id/nav_exit"
        android:name="com.fictivestudios.orbicons.fragments.HomeFragment"
        android:label="Exit"
        tools:layout="@layout/fragment_home" />
</navigation>